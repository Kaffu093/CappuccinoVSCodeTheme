	// Token Colors (https://macromates.com/manual/en/language_grammars)
	"tokenColors": [
		{
			"name": "Text",
			"scope": ["text"],
			"settings": {
				"foreground": "#A1BBEC"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"comment.line",
				"comment.line.double-slash",
				"comment.line.double-dash",
				"comment.line.number-sign",
				"comment.line.percentage",
				"comment.line.character",
				"comment.block",
				"comment.block.documentation",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#18599C"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"constant.numeric",
				"constant.character",
				"constant.character.escape",
				"constant.language",
				"constant.other",
				"string.interpolated",
				"string.regexp",
				"support.constant",
				"markup.underline.link",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#71C33E"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity",
