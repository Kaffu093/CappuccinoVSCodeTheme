		},
		{
			"name": "Functions",
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#D75043"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.other",
				"storage.modifier",
				"storage.type",
